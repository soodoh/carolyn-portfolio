{
  "name": "carolyn-portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000 --host",
    "build": "vite build",
    "preview": "vite preview --port 4000",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:e2e": "playwright test",
    "test:unit": "vitest"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@csstools/postcss-global-data": "^3.0.0",
    "@playwright/test": "^1.47.2",
    "@svelte-put/preprocess-inline-svg": "^2.1.4",
    "@sveltejs/adapter-netlify": "^4.3.4",
    "@sveltejs/kit": "^2.6.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/marked": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "browserslist": "^4.24.0",
    "contentful": "^11.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-svelte": "^2.44.1",
    "playwright": "^1.47.2",
    "postcss": "^8.4.47",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.0.6",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.4",
    "svelte-eslint-parser": "0.41.1",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/karla": "^5.1.0",
    "@fontsource/old-standard-tt": "^5.1.0",
    "jimp": "^1.6.0",
    "marked": "14.1.2",
    "p-all": "^5.0.0",
    "svelte-bricks": "^0.2.1"
  },
  "packageManager": "yarn@4.9.1"
}
